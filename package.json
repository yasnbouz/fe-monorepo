{
  "name": "frontend-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint --parallel",
    "typecheck": "turbo run typecheck --parallel",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,js,tsx,json}\"",
    "storybook": "turbo run storybook --parallel",
    "storybook:build": "turbo run storybook:build --parallel",
    "storybook:serve": "turbo run storybook:serve --parallel"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "1.6.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.2"
}
