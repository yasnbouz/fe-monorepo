{
  "name": "frontend-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook": "turbo run storybook --parallel",
    "storybook:build": "turbo run storybook:build --parallel",
    "storybook:serve": "turbo run storybook:serve --parallel"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "1.6.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "packageManager": "pnpm@7.18.2"
}