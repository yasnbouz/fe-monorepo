{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "storybook": "concurrently \"pnpm:watch:*\"",
    "build-storybook": "concurrently \"pnpm:build:*\"",
    "build:css": "tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css",
    "build:storybook": "storybook build --quiet",
    "watch:css": "tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css --watch",
    "watch:storybook": "storybook dev -p 6008 --no-open"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "cva": "npm:class-variance-authority@0.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "icons": "workspace:*"
  },
  "devDependencies": {
    "@capsizecss/metrics": "^0.4.0",
    "@storybook/addon-essentials": "7.0.0-beta.31",
    "@storybook/addon-interactions": "7.0.0-beta.31",
    "@storybook/addon-links": "7.0.0-beta.31",
    "@storybook/blocks": "7.0.0-beta.31",
    "@storybook/core-common": "^7.0.0-beta.31",
    "@storybook/react": "7.0.0-beta.31",
    "@storybook/react-vite": "7.0.0-beta.31",
    "@storybook/testing-library": "0.0.14-next.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "prop-types": "15.8.1",
    "storybook": "7.0.0-beta.31",
    "storybook-addon-designs": "beta",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tailwindcss-capsize": "^3.0.3",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}