    "react-dom": "^18.2.0",
    "tailwind-merge": "1.8.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.0-beta.41",
    "@storybook/addon-interactions": "7.0.0-beta.41",
    "@storybook/addon-links": "7.0.0-beta.41",
    "@storybook/blocks": "7.0.0-beta.41",
    "@storybook/react": "7.0.0-beta.41",
    "@storybook/react-vite": "7.0.0-beta.41",
    "@storybook/testing-library": "0.0.14-next.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.8.1",
    "storybook": "7.0.0-beta.41",
    "storybook-addon-designs": "beta",
    "storybook-addon-pseudo-states": "2.0.0-next.0",
    "tailwind-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
