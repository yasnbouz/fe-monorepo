{
  "name": "@hu/ui",
  "private": true,
  "version": "0.1.0",
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "storybook": "run-p watch:*",
    "build-storybook": "run-p build:*",
