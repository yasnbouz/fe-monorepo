{
  "name": "@hu/icons",
  "version": "0.1.0",
  "description": "Transform Icons into Optimized React Component",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "main": "./dist/cjs/index.js",
      "module": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "svgr": "rm -rf src && svgr -d src -- assets",
    "bundle": "tsup --onSuccess \"pnpm tsc\"",
    "build": "pnpm svgr && pnpm bundle"
  },
  "devDependencies": {
    "@svgr/cli": "6.5.1",
    "@types/react": "18.0.27",
    "react": "18.2.0",
    "tsup": "6.6.0",
    "tsconfig": "workspace:*"
  },
  "pnpm": {
    "overrides": {
      "svgo": "3.0.2"
    }
  }
}
